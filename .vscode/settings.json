{
    "cSpell.language": "en,en-US",
    "python.pythonPath": "${workspaceFolder}\\xleapp\\.venv\\Scripts\\python.exe",
    "cSpell.words": [
        "Brignoni",
        "LEAPP",
        "automodule",
        "xleapp",
        "isabstract",
        "recommonmark",
        "rmfile",
        "sqlitedb",
        "toctree"
    ],
    "python.linting.pylintEnabled": false,
    "python.linting.flake8Enabled": true,
    "python.linting.enabled": true,
    "python.formatting.provider": "black",
    "restructuredtext.confPath": "${workspaceFolder}\\xleapp\\docs\\source",
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 90,
    "editor.rulers": [
        90,
        120
    ],
    "workbench.colorCustomizations": {
        "editorRuler.foreground": "#ff4081"
    },
    "cornflakes.linter.executablePath": "${workspaceFolder}\\xleapp\\.venv\\Scripts\\flakehell.exe",
    "editor.formatOnSave": true,
}